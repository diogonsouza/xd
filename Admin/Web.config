      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
  <!-- permite o acesso de usuários não logados à pasta fonts -->
  <location path="fonts">
    <system.web>
      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>

  <!-- permite o acesso de usuários não logados ao /Default.aspx -->
  <location path="Default.aspx">
